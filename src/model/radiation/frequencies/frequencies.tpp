accel inline void Frequencies :: set_npoints (const Size n)
{
    npoints = n;
}


accel inline Size Frequencies :: get_npoints () const
{
    return npoints;
}


accel inline void Frequencies :: set_nquads (const Size n)
{
    nquads = n;
}


accel inline Size Frequencies :: get_nquads () const
{
    return nquads;
}


accel inline void Frequencies :: set_nlines (const Size n)
{
    nlines = n;
}


accel inline Size Frequencies :: get_nlines () const
{
    return nlines;
}
